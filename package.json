{
  "name": "react-starter",
  "version": "1.0.0",
  "author": "Artem Sitnikov <seestein9@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:watch": "babel-node src/tools/start.ts --extensions \".ts,.tsx\"",
    "start:production": "node build/server.js",
    "build": "babel-node src/tools/build.ts --extensions \".ts,.tsx\" --release",
    "bundle-report": "babel-node src/tools/build.ts --extensions \".ts,.tsx\" --release --stats",
    "ts-type-check": "tsc --noEmit",
    "lint-js": "eslint --ext .ts --ext .tsx \"src/\" --quiet",
    "lint-css": "stylelint \"src/**/*.css\"",
    "lint": "yarn run lint-js && yarn run lint-css"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "tsc --noEmit && lint-staged",
      "post-checkout": "yarn"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext .ts --ext .tsx \"src/\" --quiet"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.4.0",
    "@types/body-parser": "1.19.0",
    "@types/classnames": "2.2.10",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.8",
    "@types/express-http-proxy": "1.6.1",
    "@types/express-request-id": "1.4.1",
    "@types/serialize-javascript": "4.0.0",
    "body-parser": "1.19.0",
    "classnames": "2.2.6",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "express-request-id": "1.4.1",
    "history": "5.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "serialize-javascript": "5.0.1",
    "universal-router": "9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/assets-webpack-plugin": "3.9.0",
    "@types/html-webpack-plugin": "3.2.3",
    "@types/mini-css-extract-plugin": "1.0.0",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/webpack-bundle-analyzer": "3.8.0",
    "@types/webpack-dev-middleware": "3.7.2",
    "@types/webpack-env": "1.15.2",
    "@types/webpack-hot-middleware": "2.25.3",
    "@types/webpack-node-externals": "2.5.0",
    "@types/write-file-webpack-plugin": "4.5.0",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.3.0",
    "assets-webpack-plugin": "6.0.3",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "cache-loader": "4.1.0",
    "css-loader": "4.3.0",
    "cssnano": "4.1.10",
    "eslint": "7.10.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
