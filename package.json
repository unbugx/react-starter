{
  "name": "react-starter",
  "version": "0.0.0",
  "author": "Artem Sitnikov <seestein9@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:watch": "babel-node src/tools/start.ts --extensions \".ts,.tsx\"",
    "start:production": "node build/server.js",
    "start:test": "APP_PORT=3051 APP_BASE_PATH='/some' babel-node src/tools/start.ts --extensions \".ts,.tsx\"",
    "build": "babel-node src/tools/build.ts --extensions \".ts,.tsx\" --release",
    "bundle-report": "babel-node src/tools/build.ts --extensions \".ts,.tsx\" --release --stats",
    "release": "standard-version",
    "test:watch": "start-server-and-test start:test http://localhost:3052 cypress:open",
    "test": "start-server-and-test start:test http://localhost:3052 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "ts-type-check": "tsc --noEmit",
    "lint-js": "eslint --ext .ts --ext .tsx \"src/\" --quiet",
    "lint-css": "stylelint \"src/**/*.css\"",
    "lint": "yarn run lint-js && yarn run lint-css"
  },
  "resolutions": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "tsc --noEmit && lint-staged",
      "post-checkout": "yarn"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext .ts --ext .tsx \"src/\" --quiet"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.5.0",
    "body-parser": "1.19.0",
    "classnames": "2.2.6",
    "cookie-parser": "1.4.5",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "express-request-id": "1.4.1",
    "history": "5.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "serialize-javascript": "5.0.1",
    "universal-router": "9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/node": "7.12.10",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@cypress/webpack-preprocessor": "5.5.0",
    "@types/assets-webpack-plugin": "6.0.0",
    "@types/body-parser": "1.19.0",
    "@types/classnames": "2.2.11",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.9",
    "@types/express-http-proxy": "1.6.1",
    "@types/express-request-id": "1.4.1",
    "@types/html-webpack-plugin": "3.2.4",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.14",
    "@types/rimraf": "3.0.0",
    "@types/serialize-javascript": "5.0.0",
    "@types/webpack-bundle-analyzer": "3.9.0",
    "@types/webpack-dev-middleware": "3.7.2",
    "@types/webpack-env": "1.16.0",
    "@types/webpack-hot-middleware": "2.25.3",
    "@types/write-file-webpack-plugin": "4.5.0",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "assets-webpack-plugin": "7.0.0",
    "autoprefixer": "10.1.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "cache-loader": "4.1.0",
    "css-loader": "5.0.1",
    "cssnano": "4.1.10",
    "cypress": "6.2.0",
    "cypress-pipe": "2.0.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
