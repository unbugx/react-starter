    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.6",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-style-loader": "5.1.0",
    "lint-staged": "10.5.3",
    "postcss": "8.2.1",
